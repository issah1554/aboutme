{
    "name": "Protect Main",
    "target": "branch",
    "enforcement": "active",
    "conditions": {
        "ref_name": {
            "include": [
                "refs/heads/main"
            ],
            "exclude": []
        }
    },
    "rules": [
        {
            "type": "non_fast_forward"
        },
        {
            "type": "deletion"
        },
        {
            "type": "pull_request"
        }
    ]
}